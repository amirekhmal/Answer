import java.util.Arrays;
public class sort
{
    public static void main(String args[]){
     int [] array = {100, 2, 5, 13, 29, 68, 87, 4, 17, 45, 54}; //array that contain several numbers
     Arrays.sort(array);                                        // sorting numbers by ascending
     for (int i = 0; i < array.length; i++) {
         System.out.println(array[i]);                          // print numbers by ascending
    }
  }
}
